<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="PickMeal.PickMeal.mapper.TwentyQuestionsMapper">

    <select id="getNextValidQuestion" resultType="PickMeal.PickMeal.domain.Questions">
        SELECT * FROM questions
        WHERE attribute_name NOT IN
        <foreach item="used" collection="usedAttributes" open="(" separator="," close=")">
            #{used}
        </foreach>
        LIMIT 1
    </select>

</mapper>